    Title = "Show FOV", 
    Default = true,
    Callback = function(state)
        getgenv().ShowFOV = state
        updateFOVCircle()
    end
})

local FOVSlider = Tabs.Combat:Slider({
    Title = "FOV Size",
    Step = 5,
    Value = {Min = 30, Max = 200, Default = 80},
    Callback = function(value)
        getgenv().FOVSize = value
        updateFOVCircle()
    end
})

