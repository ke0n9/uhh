    Title = "Aim Smoothing",
    Step = 1,
    Value = {Min = 1, Max = 10, Default = 3},
    Callback = function(value)
        getgenv().AimSmoothing = value
    end
})

local AimKeyDropdown = Tabs.Combat:Dropdown({
    Title = "Aim Key",
    Values = {"MouseButton2", "LeftShift", "LeftControl", "Q", "E"},
    Value = "MouseButton2",
    Callback = function(option)
        getgenv().AimKey = option
    end
})

Tabs.Combat:Section({Title = "FOV"})

